<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RootServlet</servlet-name>
  	<display-name>RootServlet</display-name>
  	<description></description>
  	<servlet-class>org.fiteagle.dm.intercloud.RootServlet</servlet-class>
  	<load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>QueryEndPointServlet</servlet-name>
  	<display-name>QueryEndPointServlet</display-name>
  	<description></description>
  	<servlet-class>org.fiteagle.dm.intercloud.QueryEndPointServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RootServlet</servlet-name>
  	<url-pattern>/monitor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>QueryEndPointServlet</servlet-name>
  	<url-pattern>/QueryEndPointServlet</url-pattern>
  </servlet-mapping>
</web-app>
